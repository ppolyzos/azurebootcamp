<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Location - Global Azure Bootcamp 2016</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="keywords" content="athens, greece, azure, bootcamp, Paris Polyzos, Kostas Pantos, Aleksandros Fatles, Vasilis Aivailis, Stavros Menegos, Antonis Chatzipavlis" />
    <meta name="description" content="One-day event held on April 16th in Athens, Greece, where everyone, globally, talks about the expanding universe of Microsoft Azure!" />

    <meta property="og:url" content="http://athens.azurebootcamp.net/" />
    <meta property="og:type" content="website" />
    <meta property="og:locale" content="en_US" />
    <meta property="og:site_name" content="Athens Azure Bootcamp" />
    <meta property="og:title" content="Athens Azure Bootcamp" />
    <meta property="og:description" content="One-day event held on April 16th in Athens, Greece, where everyone, globally, talks about the expanding universe of Microsoft Azure!" />
    <meta property="og:image" content="http://athens.azurebootcamp.net/athens-greece/logo.png" />


    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta property="og:title" content="@ViewBag.Location - Global Azure Bootcamp 2016">
    <meta property="og:image" content="~/images/fb_img.png">
    <environment names="Development,Staging">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.css" />
        <link rel="stylesheet" href="~/css/animate.css">
        <link rel="stylesheet" href="~/css/slick.css">
        <link rel="stylesheet" href="~/js/rs-plugin/css/settings.css">
        <link rel="stylesheet" href="~/css/site.css" />
        <link rel="stylesheet" href="~/css/custom.css" />
        <link rel="stylesheet" href="~/css/TimeCircles.css" />

    </environment>
    <environment names="Production">
        <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous"/>
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-XdYbMnZ/QjLh6iI4ogqCTaIjrFk87ip+ekIjefZch0Y+PvJ8CDYtEs1ipDmPorQ+" crossorigin="anonymous">
        <link rel="stylesheet" href="http://fonts.googleapis.com/css?family=Lato:300,400,700,900"/>
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true"/>
    </environment>

    <script type="text/javascript">
        var appInsights=window.appInsights||function(config){
            function r(config){t[config]=function(){var i=arguments;t.queue.push(function(){t[config].apply(t,i)})}}var t={config:config},u=document,e=window,o="script",s=u.createElement(o),i,f;for(s.src=config.url||"//az416426.vo.msecnd.net/scripts/a/ai.0.js",u.getElementsByTagName(o)[0].parentNode.appendChild(s),t.cookie=u.cookie,t.queue=[],i=["Event","Exception","Metric","PageView","Trace"];i.length;)r("track"+i.pop());return r("setAuthenticatedUserContext"),r("clearAuthenticatedUserContext"),config.disableExceptionTracking||(i="onerror",r("_"+i),f=e[i],e[i]=function(config,r,u,e,o){var s=f&&f(config,r,u,e,o);return s!==!0&&t["_"+i](config,r,u,e,o),s}),t
        }({
            instrumentationKey:"5befa4f8-7959-4cf9-8ba9-52250c630ad8"
        });

        window.appInsights=appInsights;
        appInsights.trackPageView();
    </script>
</head>
<body>
    <header>
        <nav class="navbar navbar-default navbar-fixed-top" role="navigation">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1"> <span class="fa fa-bars fa-lg"></span> </button>
                    <a class="navbar-brand" href="/"> <img src="~/images/logo.png" alt="" class="logo"> </a>
                </div>
                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <a href="#about">about</a>
                        </li>
                        @RenderSection("Menu", false)
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container-->
        </nav>
        <div class="text-center intro">
            <div class="intro-body">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <div id="divTimer">
                                <div id="CountDownTimer" data-bgcolor="#fff" data-date="2016-04-16 10:00:00" class="centered"
                                     style="width: 100%; height: auto;"></div>
                            </div>
                            @RenderSection("Callout", false)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </header>
    <div class="wrapper">
        <section id="about">
            <div class="container">
                <div class="section-heading scrollpoint sp-effect3">
                    <h1>About Us</h1>
                    <div class="divider"></div>
                    <p>One-day event held on <b>April 16th</b> in <b>Athens, Greece</b>, where everyone, <b><a href="//global.azurebootcamp.net" target="_blank">globally</a></b>, talks about the expanding universe of Microsoft Azure!</p>
                </div>
                <div class="row">
                    <div class="col-md-4 col-sm-4 col-xs-6">
                        <div class="about-item scrollpoint sp-effect2">
                            <i class="fa fa-cloud fa-2x"></i>
                            <h3>Deep Dive Sessions</h3>
                            <p>
                                One full day of <b>deep dive</b> sessions in Azure for Developers and IT Pro's, delivered by the experts, to get you started on complex topics like Hyper-Scalable Microservice Architecture,
                                Search, Elastic SQL Database.
                            </p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-6">
                        <div class="about-item scrollpoint sp-effect5">
                            <i class="fa fa-usd fa-2x"></i>
                            <h3>Free registration</h3>
                            <p>
                                Entry to this event is totally free! What else? <b>Lots of SWAGs</b> from our sponsors!
                            </p>
                        </div>
                    </div>
                    <div class="col-md-4 col-sm-4 col-xs-6">
                        <div class="about-item scrollpoint sp-effect5">
                            <i class="fa fa-users fa-2x"></i>
                            <h3>Discuss with Experts</h3>
                            <p> If you have any questions, if you need to discuss your technical issues, or if you are simply looking for some advice, then do attend so as to speak with the experts!</p>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        @RenderBody()
    </div>
    <footer>
        <div class="large_white_light sft" data-x="center" data-y="250" data-hoffset="0" data-voffset="0" data-speed="1000" data-start="1400" data-easing="Power4.easeOut">
            [ Designed by
            <a href="//www.ganshani.com/?ref=azurebootcamp" target="_blank">Punit Ganshani</a> Hosted on
            <a href="//azure.microsoft.com/en-us/" target="_blank">Microsoft Azure</a> Theme
            <a href="//www.scoopthemes.com/templates/Oleose/Freeze/" target="_blank">Freeze</a> ] <br />
            [ Modifications for Athens Azure Bootcamp by <a href="//ppolyzos.com/?ref=azurebootcamp" target="_blank">Paris Polyzos</a>]
        </div>
    </footer>


    <environment names="Development,Staging">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/jquery-ui/jquery-ui.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <!-- Animation JS -->
        <script src="~/js/slick.min.js" asp-append-version="true"></script>
        <script src="~/js/placeholdem.min.js"></script>
        <script src="~/js/rs-plugin/js/jquery.themepunch.plugins.min.js" asp-append-version="true"></script>
        <script src="~/js/rs-plugin/js/jquery.themepunch.revolution.min.js" asp-append-version="true"></script>
        <script src="~/js/waypoints.min.js" asp-append-version="true"></script>

        <!--Custom JS-->

        <script src="~/js/TimeCircles.js" asp-append-version="true"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="~/js/custom.js" asp-append-version="true"></script>
        <script src="~/js/google.js" asp-append-version="true"></script>


    </environment>

    <environment names="Production">
        <script src="https://code.jquery.com/jquery-2.1.4.min.js" type="text/javascript"></script>
        <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js" type="text/javascript"></script>
        <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous" type="text/javascript"></script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVwXtP3QfO-26Q58ziugKdf5qjyNccI7g" type="text/javascript"></script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>

        <script>
            (function (i, s, o, g, r, a, m) {
                i['GoogleAnalyticsObject'] = r;
                i[r] = i[r] || function () {
                    (i[r].q = i[r].q || []).push(arguments)
                }, i[r].l = 1 * new Date();
                a = s.createElement(o),
                    m = s.getElementsByTagName(o)[0];
                a.async = 1;
                a.src = g;
                m.parentNode.insertBefore(a, m)
            })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

            ga('create', 'UA-73820550-1', 'auto');
            ga('send', 'pageview');

        </script>

    </environment>

    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            window['appMaster'].preLoader();
        });
    </script>
</body>
</html>
